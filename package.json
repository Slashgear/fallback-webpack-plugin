{
  "name": "@slashgear/fallback-webpack-plugin",
  "version": "1.0.6-2",
  "main": "src/index.js",
  "repository": "https://github.com/Slashgear/fallback-webpack-plugin",
  "author": "Slashgear <antoine395.caron@gmail.com>",
  "license": "MIT",
  "keywords": [
    "webpack",
    "plugin",
    "code splitting",
    "fallback",
    "replacement",
    "configuration"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "lint": "yarn prettier --list-different '{src,test}/**/*.js' '*.md'",
    "build:a": "TYPE=A webpack --profile --json > bundleA.json",
    "build:b": "TYPE=B webpack --profile --json > bundleB.json",
    "build:c": "TYPE=C webpack --profile --json > bundleC.json",
    "pretest": "run-s build:*",
    "test": "node test/test.js"
  },
  "devDependencies": {
    "husky": "1.3.1",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "prettier": "1.15.3",
    "pretty-quick": "^1.9.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
